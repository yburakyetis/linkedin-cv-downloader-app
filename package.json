{
  "name": "linkedin-cv-downloader-app",
  "version": "1.0.0",
  "description": "LinkedIn CV Downloader - Electron application for automated CV downloads from LinkedIn job applicants",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "test": "jest"
  },
  "keywords": [
    "linkedin",
    "automation",
    "cv",
    "electron",
    "playwright"
  ],
  "author": "Yusuf Burak Yetis",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "playwright": "^1.57.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0"
  },
  "build": {
    "appId": "com.ugurakkurt.linkedin-cv-downloader",
    "productName": "LinkedIn CV Downloader",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.business"
    },
    "win": {
      "target": "nsis"
    }
  }
}